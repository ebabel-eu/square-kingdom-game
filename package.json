{
  "name": "square-kingdom-mmo",
  "version": "1.0.0",
  "description": "This is a persistent, multiplayer world of tiles, where each player tries to expand his territory of tiles from one starting tile to more tiles conquered from his neighbours.",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run build",
    "build": "node ./node_modules/.bin/webpack -p --config ./webpack.config.js --progress --colors",
