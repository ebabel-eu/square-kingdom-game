    "start": "npm run build && node ./node_modules/.bin/http-server ./",
    "dev": "node ./node_modules/.bin/webpack -w"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/nadjibamar/square-kingdom-mmo.git"
  },
